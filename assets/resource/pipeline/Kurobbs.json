    "SupplyPNS": {
        "recognition": {
            "type": "ColorMatch",
            "param": {
                "lower": [220, 200, 70],
                "upper": [240, 220, 150],
                "connected": true,
                "count": 55
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        },
        "next": []
    },
    "SignZonePNS": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": ["领取记录"]
            }
        },
        "action": {
            "type": "Swipe",
            "param": {
                "begin": [90, 800, 80, 80],
                "end": [90, 200, 80, 80],
                "duration": 500
