{
    "PNS": {
        "post_wait_freezes": 500,
        "next": [
            "DailySupplyPNS"
        ],
        "interrupt": [
            "SupplyDone",
            "ExitSupplyPage",
            "AD",
            "Update",
            "ChildMode",
            "PopUp",
            "ErrorPopup",
            "TopbarPNS",
            "HomeBar",
            "StartAPPKuroBBS"
        ]
    },
    "MC": {
        "post_wait_freezes": 500,
        "next": [
            "DailySupplyMC"
        ],
        "interrupt": [
            "SupplyDone",
            "ExitSupplyPage",
            "AD",
            "Update",
            "ChildMode",
            "PopUp",
            "ErrorPopup",
            "TopbarMC",
            "HomeBar",
            "StartAPPKuroBBS"
        ]
    },
    "StartAPPKuroBBS" :{
        "action": {
            "type": "StartApp",
            "param": {
                "package": "com.kurogame.kjq/com.kurogame.kjq.userlogin.ui.activity.SplashActivity"
            }
        }
    },
    "StopAPPKuroBBS" :{
        "action": {
            "type": "StopApp",
            "param": {
                "package": "com.kurogame.kjq"
            }
        },
        "next": []
    },
    "SupplyDone": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": ["签到成功"],
                "roi": [250, 250, 250, 80]
            }
        },
        "action": {
            "type": "ClickKey",
            "param": {
                "key": [4]
            }
        }
    },
    "ExitSupplyPage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": [
                    "《战双帕弥什》每日签到",
                    "《鸣潮》每日签到工具",
                    "每日签到"
                ],
                "roi": [0, 100, 720, 60]
            }
        },
        "action": {
            "type": "ClickKey",
            "param": {
                "key": [4]
            }
        }
    },
    "AD": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": ["跳过"],
                "roi": [500, 100, 200, 150]
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        }
    },
    "Update": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": ["暂不更新"],
                "roi": [100, 800, 250, 100]
            }
