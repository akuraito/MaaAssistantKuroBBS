        "action": {
            "type": "Click",
            "param": {}
        }
    },
    "PopUp": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "PopUpClose.png",
                "roi": [600, 40, 100, 100]
            }
        },
        "action": {
            "type": "Click",
            "param": {}
        }
    },
    "HomeBar": {
        "recognition": {
